<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/toolbar" />

    <androidx.viewpager.widget.ViewPager
        android:layout_width="wrap_content"
        android:layout_height="230dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/viewPager" />

    <Button
        android:id="@+id/main_jobinform"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="취업에 도움이 될 만한 정보"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/extext"
        app:layout_constraintVertical_bias="0.186"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/main_community"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="정보나눔커뮤니티"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/main_jobinform"
        app:layout_constraintVertical_bias="0.27"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/main_interview"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="면접질문 리스트"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/main_community"
        app:layout_constraintVertical_bias="0.36"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/main_suitloan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="정장 무료 대여"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/main_interview"
        app:layout_constraintVertical_bias="0.618"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>